<resources>
	<style name="title_text">
		<item name="android:textColor">#056CB4</item>
	</style>
	<style name="title_text.non_read_message">
		<item name="android:textStyle">bold</item>
	</style>
	<style name="read_message">
		<item name="android:textStyle">normal</item>
		<item name="android:textColor">#14466F</item>
	</style>
	<style name="activities">
		<item name="android:background">@drawable/list_background</item>
	</style>
	<style name="shadowed_button">
		<item name="android:shadowColor">#444444</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">9</item>
	</style>
	<style name="title_text.settings_title">
		<item name="android:textSize">30sp</item>
		<item name="android:layout_marginLeft">10dp</item>
		<item name="android:shadowColor">#99FFFFFF</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">0</item>
		<item name="android:shadowRadius">6</item>
		<item name="android:layout_marginTop">6dp</item>
	</style>
	<style name="settings_label">
		<item name="android:textColor">#000000</item>
		<item name="android:paddingTop">15dp</item>
		<item name="android:layout_marginLeft">10dp</item>
	</style>
	<style name="settings_input">
		<item name="android:textColor">#000000</item>
	</style>
	<style name="button">
		<item name="android:background">@drawable/button</item>
	</style>
</resources>